[org.seedstack.seed]
mail.providers= smtp, smtp2, imap, pop3

[org.seedstack.seed.mail.provider.smtp.property]
mail.transport.protocol = smtp
mail.smtp.host = testserver
mail.smtp.auth = true
mail.smtp.user = testuser
mail.smtp.password = testpw

[org.seedstack.seed.mail.provider.smtp2.property]
mail.transport.protocol = smtp
mail.smtp.host = testserver2
mail.smtp.auth = true
mail.smtp.user = testuser2
mail.smtp.password = testpw2

[org.seedstack.seed.mail.provider.imap.property]
mail.store.protocol = imap
mail.imap.user = toto_user@ext.mpsa.com
mail.imap.host = testserver3
mail.imap.auth.login.disable = false
mail.imap.auth.plain.disable = true

[org.seedstack.seed.mail.provider.pop3.property]
mail.store.protocol = pop3
mail.pop3.host = testserver4
